{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "source": {
    "includePattern": ".+\\.ts$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/jsdoc-babel"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  },
  "babel": {
    "extensions": ["ts"],
    "ignore": ["**/*.d.ts"],
    "babelrc": false,
    "presets": [
      ["@babel/preset-typescript"]
    ]
  }
}